.list = res;\n        this.listcars = this.list.cars;\n        this.page = this.list.page;\n        this.pagePrev = this.list.page_prev;\n        this.pageNext = this.list.page_next;\n      },err=>{\n        console.log(err);\n      }\n    );\n  }\n  ngOnInit() {\n  }\n}\n"]}