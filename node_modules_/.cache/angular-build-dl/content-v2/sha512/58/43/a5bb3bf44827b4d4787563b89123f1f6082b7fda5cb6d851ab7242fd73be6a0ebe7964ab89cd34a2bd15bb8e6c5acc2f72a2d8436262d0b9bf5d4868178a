/***/ "./src/app/car-add/car-add.page.ts":
/*!*****************************************!*\
  !*** ./src/app/car-add/car-add.page.ts ***!
  \*****************************************/
/*! exports provided: CarAddPage */
/***/ (function(module, __webpack_exports__, __webpack_require__) {

"use strict";
__webpack_require__.r(__webpack_exports__);
/* harmony export (binding) */ __webpack_require__.d(__webpack_exports__, "CarAddPage", function() { return CarAddPage; });
/* harmony import */ var tslib__WEBPACK_IMPORTED_MODULE_0__ = __webpack_require__(/*! tslib */ "./node_modules/tslib/tslib.es6.js");
/* harmony import */ var _angular_core__WEBPACK_IMPORTED_MODULE_1__ = __webpack_require__(/*! @angular/core */ "./node_modules/@angular/core/__ivy_ngcc__/fesm2015/core.js");
/* harmony import */ var _rest_api_service__WEBPACK_IMPORTED_MODULE_2__ = __webpack_require__(/*! ../rest-api.service */ "./src/app/rest-api.service.ts");
/* harmony import */ var _ionic_storage__WEBPACK_IMPORTED_MODULE_3__ = __webpack_require__(/*! @ionic/storage */ "./node_modules/@ionic/storage/__ivy_ngcc__/fesm2015/ionic-storage.js");




let CarAddPage = class CarAddPage {
    constructor(api, storage) {
        this.api = api;
        this.storage = storage;
        this.cardata = {
            type_id: '',
            brand_id: '',
            generation_id: '',
            face_id: '',
            model_id: '',
            year_id: '',
            cc_id: '',
            gear_id: '',
            mile: '',
            color: '',
            price: '',
            license: '',
            detail: '',
            image: '',
            yb: ''
        };
        this.api.getdata('cars/getListType').subscribe(res => { this.listtype = res; });
        this.api.getdata('cars/getListBand').subscribe(res => {
            this.listbrand = res;
        });
        this.api.getdata('cars/getListYear').subscribe(res => {
            this.listyear = res;
        });
        this.api.getdata('cars/getListCC').subscribe(res => {
            this.listcc = res;
        });
        this.api.getdata('cars/getListGear').subscribe(res => {
            this.listgear = res;
        });
    }
    getGen($event) {
        this.brand_id = $event.target.value;
        this.api.getdata('cars/getLlistGeneration&brand_id=' + this.brand_id).subscribe(res => {
            this.listgen = res;
        });
    }
    getFace($event) {
        this.model_id = $event.target.value;
        this.api.getdata('cars/getListFace&brand_id=' + this.brand_id + '&model_id=' + this.model_id).subscribe(res => {
            this.listface = res;
            this.api.getdata('cars/getListModel&brand_id=' + this.brand_id + '&model_id=' + this.model_id).subscribe(res => {
                this.listmodel = res;
            });
        });
    }
    formcar(form) {
        this.storage.get('token').then((data) => {
            this.token = data;
            // this.title = form.value.year_id+" "+form.value.brand_id+" "+form.value.generation_id;
            this.api.getdata('cars/getBand&id=' + form.value.brand_id).subscribe(res => {
                this.val_brand = res;
                this.api.getdata('cars/getGeneration&id=' + form.value.generation_id).subscribe(res => {
                    this.val_gen = res;
                    this.title = form.value.year_id + " " + this.val_brand.text + " " + this.val_gen.text;
                    // console.log(this.title);
                    this.api.getdata('cars/addCar&token=' + this.token + '&title=' + this.title + '&type_id=' + form.value.type_id + '&brand_id=' + form.value.brand_id + '&generation_id=' + form.value.generation_id + '&face_id=' + form.value.face_id + '&model_id=' + form.value.model_id + '&year_id=' + form.value.year_id + '&cc_id=' + form.value.cc_id + '&gear_id=' + form.value.gear_id + '&mile=' + form.value.mile + '&color=' + form.value.color + '&price=' + form.value.price + '&license=' + form.value.license + '&detail=' + form.value.detail + '&image=' + form.value.image).subscribe(res => {
                        console.log(res);
                    }, err => {
                        console.log(err);
                    });
                });
            });
        });
    }
    ngOnInit() {
    }
};
CarAddPage.ctorParameters = () => [
    { type: _rest_api_service__WEBPACK_IMPORTED_MODULE_2__["RestApiService"] },
    { type: _ionic_storage__WEBPACK_IMPORTED_MODULE_3__["Storage"] }
];
CarAddPage = Object(tslib__WEBPACK_IMPORTED_MODULE_0__["__decorate"])([
    Object(_angular_core__WEBPACK_IMPORTED_MODULE_1__["Component"])({
        selector: 'app-car-add',
        template: Object(tslib__WEBPACK_IMPORTED_MODULE_0__["__importDefault"])(__webpack_require__(/*! raw-loader!./car-add.page.html */ "./node_modules/raw-loader/dist/cjs.js!./src/app/car-add/car-add.page.html")).default,
        styles: [Object(tslib__WEBPACK_IMPORTED_MODULE_0__["__importDefault"])(__webpack_require__(/*! ./car-add.page.scss */ "./src/app/car-add/car-add.page.scss")).default]
    })
], CarAddPage);



/***/ })

