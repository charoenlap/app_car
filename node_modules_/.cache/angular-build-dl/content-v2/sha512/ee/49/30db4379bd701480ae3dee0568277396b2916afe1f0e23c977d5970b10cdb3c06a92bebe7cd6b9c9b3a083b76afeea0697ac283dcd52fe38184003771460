let NewsDetailPage = class NewsDetailPage {
    constructor(api, route) {
        this.api = api;
        this.route = route;
        this.news_id = this.route.snapshot.paramMap.get('id');
        this.api.getdata('information/getNewsDetail&news_id=' + this.news_id).subscribe(res => {
            this.news = res;
            this.image = this.news.image;
            this.title = this.news.title;
            this.date = this.news.date;
            this.detail = this.news.detail;
            // loading.dismiss();
        }, err => {
            console.log(err);
            // loading.dismiss();
        });
    }
    ngOnInit() {
    }
};
NewsDetailPage.ctorParameters = () => [
    { type: _rest_api_service__WEBPACK_IMPORTED_MODULE_2__["RestApiService"] },
    { type: _angular_router__WEBPACK_IMPORTED_MODULE_3__["ActivatedRoute"] }
];
